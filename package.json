{
  "name": "create-express-app",
  "version": "1.0.0",
  "description": "create-express-app",
  "license": "MIT",
  "author": "lifegit",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development TZ=Asia/Shanghai nodemon --watch 'src/' --watch 'conf/.dev.toml' -e ts,toml --exec 'ts-node' ./src/main",
    "format": "prettier --cache --write src",
    "lint": "eslint .",
    "prepare": "husky install",
    "prisma-format": "prisma format",
    "prisma-generate": "prisma generate",
    "prisma-migrate": "prisma migrate dev",
    "prod": "cross-env NODE_ENV=production TZ=Asia/Shanghai node ./dist/main",
    "start": "cross-env NODE_ENV=development TZ=Asia/Shanghai ts-node ./src/main"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.9.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "svg-captcha": "^1.4.0",
    "toml": "^3.0.0",
    "typescript": "^4.1.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@umijs/fabric": "^4.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1"
  }
}
